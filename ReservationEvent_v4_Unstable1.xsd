	<xs:element name="ReservationServiceId" type="xs:string">
		<xs:annotation>
			<xs:documentation>The identifier, unique within the reservation, identifying the service reserved.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="SubServiceId" type="xs:string">
