															<xs:documentation>The Id of a passenger which the insurance is valid for.</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="TotalPriceAmount" type="xs:decimal" minOccurs="0">
