	<xs:element name="ProductConfiguration" type="Reference">
		<xs:annotation>
			<xs:documentation>Points out the product confiiguration for which this reservation part/service was derived from.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Product" type="Reference">
		<xs:annotation>
			<xs:documentation>Points out the original static product definition as identified by the original digital source.</xs:documentation>
